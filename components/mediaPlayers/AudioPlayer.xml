<?xml version="1.0" encoding="utf-8"?>
<component name="AudioPlayer" extends="Video">
  <children>
    <timer id="playbackTimer" repeat="true" duration="30" />
  </children>
  <interface>
    <field id="id" type="string" />
    <field id="loopMode" type="string" value="" />
    <field id="audioData" type="array" alwaysNotify="true" />
    <function name="playNext" />
    <function name="playPrevious" />
  </interface>
</component>