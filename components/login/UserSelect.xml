<?xml version="1.0" encoding="utf-8"?>
<component name="UserSelect" extends="JFScreen">
  <children>
    <Label
      text="Select User"
      horizAlign="center"
      font="font:LargeSystemFont"
      height="100"
      width="1920"
      translation="[0, 200]" />

    <UserRow id="userRow" rowFocusAnimationStyle="fixedFocusWrap" translation="[130, 360]" />

    <StandardButton
      id="manualLogin"
      text="Manual Login"
      height="85"
      width="350"
      translation="[785,850]" />
  </children>
  <interface>
    <field id="forgetUser" type="string" alwaysNotify="true" />
    <field id="itemSelected" type="string" alwaysNotify="true" />
    <field id="itemContent" type="array" onChange="itemContentChanged" />
  </interface>
</component>