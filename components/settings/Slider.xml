<?xml version="1.0" encoding="utf-8"?>
<component name="Slider" extends="Group">
  <children>
    <Rectangle id="seekBar" width="1050" height="10">
      <LayoutGroup id="optionMarkers" layoutDirection="horiz" horizAlignment="left" translation="[0, -20]" />
      <Poster id="thumb" width="25" height="25" uri="pkg:/images/icons/circle.png" translation="[-10, -10]" />
    </Rectangle>
  </children>
  <interface>
    <field id="value" type="integer" onChange="onValueChange" />
    <field id="options" type="array" onChange="onOptionsChange" />
  </interface>
</component>